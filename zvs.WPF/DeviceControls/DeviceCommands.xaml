<UserControl x:Class="zvs.WPF.DeviceControls.DeviceCommands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Loaded="UserControl_Loaded_1" Unloaded="UserControl_Unloaded_1">
    <StackPanel>
        <GroupBox Name="TypeGrpBx" Header="Device Type Commands" MinHeight="100" Margin="5,5,5,10">
            <StackPanel  Name="TypeCommandsStkPnl" Margin="0,20,0,10"></StackPanel>
        </GroupBox>
        <GroupBox Name="DeviceGrpBx" Header="Device Specific Commands" MinHeight="100" Margin="5,0,5,5" >
            <StackPanel  Name="DeviceCommandsStkPnl" Margin="0,20,0,10"></StackPanel>
        </GroupBox>
        
    </StackPanel>
</UserControl>
