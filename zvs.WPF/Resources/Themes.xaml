    <SolidColorBrush x:Key="SettingSelectedBrush" Color="{DynamicResource SettingSelectedColor}" />
    <SolidColorBrush x:Key="SettingNoFocusBrush" Color="{DynamicResource SettingNoFocusColor}" />
